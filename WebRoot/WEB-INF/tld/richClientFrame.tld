		    <required>false</required>
		    <rtexprvalue>true</rtexprvalue>
		</attribute> 
	</tag> 
	
	<tag>  
		<name>err</name>  
		<tag-class>com.richClientFrame.util.tag.widget.ErrMessageTag</tag-class>  
		<attribute>
		    <name>id</name>
		    <required>false</required>
		    <rtexprvalue>true</rtexprvalue>
		</attribute> 
		<attribute>
