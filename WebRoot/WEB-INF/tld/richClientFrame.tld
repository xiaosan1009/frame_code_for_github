		    <required>false</required>
		    <rtexprvalue>true</rtexprvalue>
		</attribute> 
		<attribute>
		    <name>style</name>
		    <required>false</required>
		    <rtexprvalue>true</rtexprvalue>
		</attribute> 
		<attribute>
		    <name>msStyle</name>
		    <required>false</required>
		    <rtexprvalue>true</rtexprvalue>
		</attribute> 
		<attribute>
		    <name>message</name>
		    <required>false</required>
		    <rtexprvalue>true</rtexprvalue>
		</attribute> 
	</tag>  
	<!-- 普通标签部分 结束 -->
</taglib>