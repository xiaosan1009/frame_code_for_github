	<xs:simpleType name="dataFormatPropertyType">
		<xs:restriction base="xs:string">
			<xs:enumeration value=""/>
			<xs:enumeration value="yyyy-MM-dd"/>
			<xs:enumeration value="yyyy/MM/dd"/>
			<xs:enumeration value="yyyyMMdd"/>
			<xs:enumeration value="yyyy年MM月dd日"/>
			<xs:enumeration value="yyyy年MM月dd日(E)"/>
			<xs:enumeration value="yyyy-MM-dd HH:mm:ss"/>
			<xs:enumeration value="yyyyMMddHHmmss"/>
			<xs:enumeration value="HH:mm:ss"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="excelCellValuePropertyType">
		<xs:restriction base="xs:string">
			<xs:enumeration value=""/>
			<xs:enumeration value="%index%"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="styleProperteValueType">
		<xs:restriction base="xs:string">
			<xs:enumeration value=""></xs:enumeration>
			<xs:enumeration value="*"></xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="targetIdPropertyType">
		<xs:restriction base="xs:string">
			<xs:enumeration value=""/>
			<xs:enumeration value="_value"/>
			<xs:enumeration value="value"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="styleProperteValueAllType">
		<xs:union memberTypes="styleProperteValueType xs:string"></xs:union>
	</xs:simpleType>
	
	<xs:simpleType name="excelCellValuePropertyAllType">
		<xs:union memberTypes="excelCellValuePropertyType xs:string"></xs:union>
	</xs:simpleType>
	
	<xs:simpleType name="targetIdPropertyAllType">
		<xs:union memberTypes="resultValueType targetIdPropertyType xs:string"></xs:union>
	</xs:simpleType>
	
	<xs:simpleType name="resultValueType">
		<xs:restriction base="xs:string">
			<xs:enumeration value=""/>
			<xs:enumeration value="user:userId"/>
			<xs:enumeration value="user:userLogin"/>
			<xs:enumeration value="user:userName"/>
			<xs:enumeration value="user:dataSource"/>
			<xs:enumeration value="user:provinceId"/>
			<xs:enumeration value="%sysdate%"/>
			<xs:enumeration value="%sequence%"/>
			<xs:enumeration value="%sessionId%"/>
			<xs:enumeration value="$listindex"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="resultValueAllType">
		<xs:union memberTypes="resultValueType xs:string"></xs:union>
	</xs:simpleType>
	
	<xs:simpleType name="colorPropertyAllType">
		<xs:union memberTypes="colorPropertyType xs:string"></xs:union>
	</xs:simpleType>
	
	<xs:simpleType name="headerAllType">
		<xs:union memberTypes="headerType headerIntType"></xs:union>
	</xs:simpleType>
	
	<xs:simpleType name="displayPropertyAllType">
		<xs:union memberTypes="displayPropertyType xs:string"></xs:union>
	</xs:simpleType>
	
	<xs:simpleType name="conditionOperatorType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="empty"/>
			<xs:enumeration value="notEmpty"/>
			<xs:enumeration value="=="/>
			<xs:enumeration value="!="/>
			<xs:enumeration value="＞"/>
			<xs:enumeration value="＞="/>
			<xs:enumeration value="＜"/>
			<xs:enumeration value="＜="/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="resultOperatorType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="set"/>
			<xs:enumeration value="add"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="conditionPropertyType">
		<xs:restriction base="xs:string">
			<xs:enumeration value=""/>
			<xs:enumeration value="#Result"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="conditionPropertyAllType">
		<xs:union memberTypes="conditionPropertyType resultValueType xs:string"></xs:union>
	</xs:simpleType>
	
	<xs:simpleType name="messageType">
		<xs:union memberTypes="commonMessageType messageIntType resultValueAllType"></xs:union>
	</xs:simpleType>
	
	<xs:simpleType name="messageIntType">
		<xs:restriction base="xs:int">
			<xs:pattern value="\d{0,2}"></xs:pattern>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="commonMessageType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="NOTHING_INPUT">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							没有输入的错误<br/>
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INPUT_ERROR">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							输入错误<br/>
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RANGE_ERROR">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							输入范围错误<br/>
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OVER_LENGTH">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							文字超限错误<br/>
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INPUT_INVALID">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							不能输入的文字错误<br/>
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NUM_ONLY_ERROR">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							数字的项目输入文字的错误<br/>
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TIME_DISORDER">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							时间前后验证错误<br/>
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OVERLAP">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							重复错误<br/>
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NUM_DIS_ORDER">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							数字前后验证错误<br/>
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="scopeType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="session"/>
			<xs:enumeration value="user"/>
			<xs:enumeration value="param"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="scopeAllType">
		<xs:union memberTypes="scopeType xs:string"></xs:union>
	</xs:simpleType>
	
	<xs:simpleType name="paramterPropertyValue">
		<xs:restriction base="xs:string">
			<xs:enumeration value="listindex"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="paramterPropertyAllValue">
		<xs:union memberTypes="paramterPropertyValue xs:string"></xs:union>
	</xs:simpleType>
	
	<xs:simpleType name="parameterValueType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="%sysdate"/>
			<xs:enumeration value="%sequence"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="parameterValueAllType">
		<xs:union memberTypes="parameterValueType xs:string"></xs:union>
	</xs:simpleType>
	
	<xs:simpleType name="dispCodeType">
		<xs:restriction base="xs:integer">
			<xs:pattern value="[0-9]{6}"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="classNameType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="abstractHandlerItem"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="methodNameType">
		<xs:restriction base="xs:string">
			<xs:pattern value="[a-z][a-zA-Z0-9_]*"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="classNameAllType">
		<xs:union memberTypes="classNameType methodNameType"></xs:union>
	</xs:simpleType>
	
	<xs:simpleType name="cmdCodeType">
		<xs:restriction base="xs:integer">
			<xs:pattern value="[0-9]+"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:element name="handlerclass">
		<xs:complexType>
			<xs:attribute name="name" type="classNameAllType" use="required">
				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						机能处理类名称<br/>
						格式：必须输入，首字母小写
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="method" substitutionGroup="cmdElements">
		<xs:complexType>
			<xs:attribute name="name" type="methodNameType" use="required">
				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						方法名称<br/>
						格式：必须输入，首字母小写
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
			</xs:attribute>
			
			<xs:attribute name="type" type="methodType">
				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
