		  		</xs:annotation>
  			</xs:attribute>
  			
			<xs:attribute name="oddColor" type="excelColorPropertyType">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						列表奇数行的颜色<br/>
						格式：颜色列表
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
			<xs:attribute name="evenColor" type="excelColorPropertyType">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						列表偶数行的颜色<br/>
						格式：颜色列表
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
		</xs:complexType>
    </xs:element>
	
	<xs:element name="row">
		<xs:complexType>
		
  			<xs:sequence>
  				<xs:element ref="cells" minOccurs="0" />
  			</xs:sequence>
  			
			<xs:attribute name="site" type="xs:integer" use="required">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						excel上单元格的行数【从0开始，如果是多项目，则为循环起始的行数】<br/>
						格式：必须输入，数值类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
  			<xs:attribute name="height" type="xs:integer">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						excel上单元格的高度设置【生成excel时】<br/>
						格式：数值类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
			<xs:attribute name="targetId" type="xs:string">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						对应来源数据中数组数据的key<br/>
						根据此值，可以在一个excel中传入多个数组数据，分别循环写入<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
  			<xs:attribute name="class" type="xs:string">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						对应<resource>标签中的样式表的ID<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
  			<xs:attribute name="start" type="xs:integer">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						cell循环的起始坐标，如果设置此值，需要设置end属性，并且嵌套<cell>标签只能有一个<br/>
						格式：数值类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
  			<xs:attribute name="end" type="xs:integer">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						cell循环的结束坐标，如果设置此值，需要设置end属性，并且嵌套<cell>标签只能有一个<br/>
						超过此值，就从下一行的start坐标处开始生成数据<br/>
						格式：数值类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
		</xs:complexType>
	</xs:element>
	
	<xs:element name="cells">
    	<xs:complexType>
			<xs:sequence>
				<xs:element ref="cell" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
    </xs:element>
	
	<xs:element name="cell">
		<xs:complexType>
  			<xs:sequence>
  				<xs:element ref="cellStyle" minOccurs="0" />
  				<xs:element ref="conditions" minOccurs="0"/>
  			</xs:sequence>
  			
			<xs:attribute name="site" type="xs:integer">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						excel上单元格的列数【从0开始】<br/>
						格式：数值类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
			<xs:attribute name="width" type="xs:integer">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						excel上单元格的宽度设置【生成excel时】<br/>
						格式：数值类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
			<xs:attribute name="type" type="excelCellTypePropertyType">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						excel上单元格的类型设置【生成excel时】<br/>
						默认：字符串常量【CELL_TYPE_STRING】<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
			<xs:attribute name="class" type="xs:string">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						对应<resource>标签中的样式表的ID<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
			<xs:attribute name="value" type="excelCellValuePropertyAllType">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						excel上单元格的值【生成excel时】<br/>
						默认：字符串常量<br/>
						【#】:数据库查询结果中的key<br/>
						【$】:request中的key<br/>
						【%】:session中的key<br/>
						【fmt:】:后边的key值对应多语言文件中的key<br/>
						【user:】:后边的key值对应用户信息在session中的key<br/>
						【%sysdate%】:系统当前日期<br/>
						【%sequence%】:根据日期时间及随机数生成序列<br/>
						【$listindex】:多项目数据的行序列<br/>
						【#Result】:数据库查询的整体结果<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
			<xs:attribute name="targetId" type="xs:string">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						excel上单元格的值对应画面控件的ID【读取excel时】<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
		</xs:complexType>
	</xs:element>
	
	<xs:element name="cellStyle">
		<xs:complexType>
  			<xs:all>
  				<xs:element ref="cellBorders" minOccurs="0" />
  				<xs:element ref="cellFont" minOccurs="0" />
  				<xs:element ref="cellDataFormat" minOccurs="0" />
  				<xs:element ref="cellLayout" minOccurs="0" />
  				<xs:element ref="cellGround" minOccurs="0" />
  			</xs:all>
  			
  			<xs:attribute name="id" type="xs:string">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						对应<resource>标签中的属性，次属性的值可以对应<cell>标签class属性，作为样式的引用<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="cellDataFormat">
		<xs:complexType>
		
  			<xs:attribute name="format" type="dataFormatPropertyType" use="required">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						单元格数据格式化的规则<br/>
						格式：必须输入，字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
		
  			<xs:attribute name="sourceFormat" type="xs:string">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						来源数据的日期格式【设置此属性，数据默认为日期类型】<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
		
		</xs:complexType>
	</xs:element>
	
	<xs:element name="cellLayout">
		<xs:complexType>
		
  			<xs:attribute name="align" type="excelCellAlignPropertyType">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						单元格横向布局方式<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
		
  			<xs:attribute name="vertical" type="excelCellVerticalPropertyType">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						单元格纵向布局方式<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
		
		</xs:complexType>
	</xs:element>
	
	<xs:element name="cellBorders">
		<xs:complexType>
  			<xs:all>
  				<xs:element ref="wholeBorder" minOccurs="0" />
  				<xs:element ref="leftBorder" minOccurs="0" />
  				<xs:element ref="rightBorder" minOccurs="0" />
  				<xs:element ref="topBorder" minOccurs="0" />
  				<xs:element ref="bottomBorder" minOccurs="0" />
  			</xs:all>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="dataSource">
		<xs:complexType>
  			<xs:attribute name="target" type="xs:string">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						数据源名称【对应applicationContext-ibatis.xml文件中，dataSource下的targetDataSources属性里的key数组的值】<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			<xs:attribute name="method" type="xs:string">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						切换数据源对应的方法名【对应applicationContext-ibatis.xml文件中，dataSource下的targetDataSources属性里的key数组的值】<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="cellFont">
		<xs:complexType>
		
			<xs:attribute name="name" type="xs:string">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						字体名称<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
		
			<xs:attribute name="bold">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						字体粗体样式<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
		  		<xs:simpleType>
  					<xs:restriction base="xs:string">
  						<xs:enumeration value="normal"></xs:enumeration>
  						<xs:enumeration value="bold"></xs:enumeration>
  					</xs:restriction>
  				</xs:simpleType>
  			</xs:attribute>
  			
  			<xs:attribute name="color" type="excelColorPropertyType">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						字体颜色<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
  			<xs:attribute name="italic" type="xs:boolean">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						字体是否为斜体<br/>
						格式：布尔类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
  			<xs:attribute name="strikeout" type="xs:boolean">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						字体是否加删除线<br/>
						格式：布尔类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
  			<xs:attribute name="fontHeight" type="xs:integer">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						字体高度<br/>
						格式：数值类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
  			<xs:attribute name="typeOffset">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						字体标注样式<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
		  		<xs:simpleType>
  					<xs:restriction base="xs:string">
  						<xs:enumeration value="normal"></xs:enumeration>
  						<xs:enumeration value="super"></xs:enumeration>
  						<xs:enumeration value="subscript"></xs:enumeration>
  					</xs:restriction>
  				</xs:simpleType>
  			</xs:attribute>
  			
  			<xs:attribute name="underline">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						字体下划线样式<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
		  		<xs:simpleType>
  					<xs:restriction base="xs:string">
  						<xs:enumeration value="NONE"></xs:enumeration>
  						<xs:enumeration value="SINGLE"></xs:enumeration>
  						<xs:enumeration value="DOUBLE"></xs:enumeration>
  						<xs:enumeration value="SINGLE_ACCOUNTING"></xs:enumeration>
  						<xs:enumeration value="DOUBLE_ACCOUNTING"></xs:enumeration>
  					</xs:restriction>
  				</xs:simpleType>
  			</xs:attribute>
  			
		</xs:complexType>
	</xs:element>
	
	<xs:element name="cellGround">
		<xs:complexType>
		
  			<xs:attribute name="foreColor" type="excelColorPropertyType">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						单元格前景色<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
		
  			<xs:attribute name="backColor" type="excelColorPropertyType">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						单元格后景色<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
		
  			<xs:attribute name="fill" type="excelCellGroundPropertyType" use="required">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						单元格填充方式<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="cellBorder">
		<xs:complexType>
		
  			<xs:attribute name="style" type="excelBorderStylePropertyType">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						边框样式<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
		
  			<xs:attribute name="color" type="excelColorPropertyType">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						边框颜色<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="wholeBorder">
		<xs:complexType>
  			<xs:sequence>
  				<xs:element ref="cellBorder" minOccurs="0" />
  			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="leftBorder">
		<xs:complexType>
  			<xs:sequence>
  				<xs:element ref="cellBorder" minOccurs="0" />
  			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="rightBorder">
		<xs:complexType>
  			<xs:sequence>
  				<xs:element ref="cellBorder" minOccurs="0" />
  			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="topBorder">
		<xs:complexType>
  			<xs:sequence>
  				<xs:element ref="cellBorder" minOccurs="0" />
  			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="bottomBorder">
		<xs:complexType>
  			<xs:sequence>
  				<xs:element ref="cellBorder" minOccurs="0" />
  			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
</xs:schema>