		  			]]>
    				</xs:documentation>
    			</xs:annotation>
    		</xs:attribute>

    		<xs:attribute name="operator" type="resultOperatorType" use="required">
    			<xs:annotation>
    				<xs:documentation>
    					<![CDATA[ 
						值变化的运算方法<br/>
						格式：必须输入，字符串类型
		  			]]>
    				</xs:documentation>
    			</xs:annotation>
    		</xs:attribute>
    		
    		<xs:attribute name="value" type="resultValueAllType" use="required">
    			<xs:annotation>
    				<xs:documentation>
    					<![CDATA[ 
						输出对象要变化的值<br/>
						默认：字符串常量<br/>
						【#】:数据库查询结果中的key<br/>
						【$】:request中的key<br/>
						【%】:session中的key<br/>
						【fmt:】:后边的key值对应多语言文件中的key<br/>
						【user:】:后边的key值对应用户信息在session中的key<br/>
						【%sysdate%】:系统当前日期<br/>
						【%sequence%】:根据日期时间及随机数生成序列<br/>
						【$listindex】:多项目数据的行序列<br/>
						格式：必须输入，字符串类型
		  			]]>
    				</xs:documentation>
    			</xs:annotation>
    		</xs:attribute>
    		
    		<xs:attribute name="list" type="xs:boolean">
    			<xs:annotation>
    				<xs:documentation>
    					<![CDATA[ 
						输出数据是否为数组类型【true：数组类型：false：非数组类型】<br/>
						格式：布尔类型
		  			]]>
    				</xs:documentation>
    			</xs:annotation>
    		</xs:attribute>
    	</xs:complexType>
    </xs:element>
    
    <xs:element name="checks">
    	<xs:complexType>
			<xs:sequence>
				<xs:element ref="check" maxOccurs="unbounded"/>
			</xs:sequence>
			
			<xs:attribute name="success" type="headerAllType">
    			<xs:annotation>
    				<xs:documentation>
    					<![CDATA[ 
						正确验证结果的信息code<br/>
						格式：headerAllType
		  			]]>
    				</xs:documentation>
    			</xs:annotation>
    		</xs:attribute>
			
			<xs:attribute name="error" type="headerAllType">
    			<xs:annotation>
    				<xs:documentation>
    					<![CDATA[ 
