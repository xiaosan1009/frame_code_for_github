	<xs:element name="cmdElements" abstract="true"/>
	
	<xs:element name="cmd">
 		<xs:complexType>
  			<xs:all>
				<xs:element ref="cmdElements" minOccurs="0"/>
				<xs:element ref="dataSource" minOccurs="0"/>
  			</xs:all>
 			
  			<xs:attribute name="code" type="cmdCodeType" use="required">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						方法ID<br/>
						格式：必须输入，输入值为数值型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
  			<xs:attribute name="jsp" type="xs:string">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						方法触发画面路径<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
  			<xs:attribute name="describe" type="xs:string">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						方法的注解<br/>
						格式：字符串类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
  			<xs:attribute name="logout">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						session过期时是否自动登出【不设置默认自动登出】<br/>
						0：不自动登出<br/>
						1：自动登出
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  				<xs:simpleType>
  					<xs:restriction base="xs:int">
  						<xs:enumeration value="0"></xs:enumeration>
  						<xs:enumeration value="1"></xs:enumeration>
  					</xs:restriction>
  				</xs:simpleType>
  			</xs:attribute>
  			
  			<xs:attribute name="forward" type="xs:integer">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						要迁移的画面ID<br/>
						格式：数值类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
  			<xs:attribute name="inputStream" type="xs:boolean">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						是否从request的inputStream中获取流信息<br/>
						【true：获取；false：不获取】，默认false<br/>
						如果为true不能在上传或者与request流相关的功能并用<br/>
						格式：布尔类型
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  			</xs:attribute>
  			
  			<xs:attribute name="check">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						是否进行共通验证<br/>
						0：不进行共通验证<br/>
						1：进行共通验证
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
  				<xs:simpleType>
  					<xs:restriction base="xs:int">
  						<xs:enumeration value="0"></xs:enumeration>
  						<xs:enumeration value="1"></xs:enumeration>
  						<xs:enumeration value="2"></xs:enumeration>
  					</xs:restriction>
  				</xs:simpleType>
  			</xs:attribute>
  			
  			<xs:attribute name="checkIndex">
  				<xs:annotation>
					<xs:documentation>
					<![CDATA[ 
						是否进行指定行数的验证，此属性值为设定行数的控件的【name】的值<br/>
						如果此项不设置，默认为【FcheckF】<br/>
		  			]]> 
		  			</xs:documentation>
		  		</xs:annotation>
		  		<xs:simpleType>
		  			<xs:union memberTypes="xs:string">
		  				<xs:simpleType>
		  					<xs:restriction base="xs:string">
		  						<xs:enumeration value=""></xs:enumeration>
		  						<xs:enumeration value="FcheckF"></xs:enumeration>
		  					</xs:restriction>
		  				</xs:simpleType>
		  			</xs:union>
		  		</xs:simpleType>
  			</xs:attribute>
  			
		</xs:complexType>
	</xs:element>
	
	<xs:element name="cmds">
 		<xs:complexType>
