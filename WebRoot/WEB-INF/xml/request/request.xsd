    <xs:element name="results">
    	<xs:complexType>
			<xs:sequence>
				<xs:element ref="result" maxOccurs="unbounded"/>
			</xs:sequence>
			
			<xs:attribute name="constant" type="xs:boolean">
    			<xs:annotation>
    				<xs:documentation>
    					<![CDATA[ 
						【result】标签中的结果是否为常量<br/>
						布尔类型
		  			]]>
    				</xs:documentation>
    			</xs:annotation>
    		</xs:attribute>
			
			<xs:attribute name="appoint" type="xs:boolean">
    			<xs:annotation>
    				<xs:documentation>
    					<![CDATA[ 
						是否只返回【result】标签对应的结果：【true：只返回result标签对应的数据；false或者不设置为返回全部结果数据】<br/>
						布尔类型
		  			]]>
    				</xs:documentation>
    			</xs:annotation>
    		</xs:attribute>
		</xs:complexType>
    </xs:element>

    <xs:element name="result">
    	<xs:complexType>
    		<xs:all>
   				<xs:element ref="conditions" minOccurs="0"/>
   				<xs:element ref="formats" minOccurs="0" />
  			</xs:all>
    		<xs:attribute name="targetId" type="targetIdPropertyAllType" use="required">
    			<xs:annotation>
    				<xs:documentation>
    					<![CDATA[ 
						处理结果对应画面控件的ID或者为数据库中对应字段名称<br/>
