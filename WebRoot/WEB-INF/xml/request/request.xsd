	<xs:simpleType name="excelBorderStylePropertyType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="BORDER_NONE"/>
			<xs:enumeration value="BORDER_THIN"/>
			<xs:enumeration value="BORDER_MEDIUM"/>
			<xs:enumeration value="BORDER_DASHED"/>
			<xs:enumeration value="BORDER_DOTTED"/>
			<xs:enumeration value="BORDER_THICK"/>
			<xs:enumeration value="BORDER_DOUBLE"/>
			<xs:enumeration value="BORDER_HAIR"/>
			<xs:enumeration value="BORDER_MEDIUM_DASHED"/>
			<xs:enumeration value="BORDER_DASH_DOT"/>
			<xs:enumeration value="BORDER_MEDIUM_DASH_DOT"/>
			<xs:enumeration value="BORDER_DASH_DOT_DOT"/>
