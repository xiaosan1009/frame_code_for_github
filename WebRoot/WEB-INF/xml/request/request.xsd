<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	
	<xs:simpleType name="methodType">
		<xs:restriction base="xs:integer">
			<xs:enumeration value="0"/>
			<xs:enumeration value="1"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="headerIntType">
		<xs:restriction base="xs:int">
			<xs:pattern value="[1-9]\d\d{1,}"></xs:pattern>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="headerType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="PAGE_CHANGED">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							画面跳转:-4
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SYSTEM_ERROR">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							服务器异常:-2
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BLANK">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							空白信息:-1
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="REGISTER_SUCCESS">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							登录完毕:0
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INPUT_ERROR">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							输入内容有错误:1
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="REGISTER_ERROR">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							输入内容的登录失败:2
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="READ_ERROR">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							数据的获得失败:3
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NO_DATA">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							不存在所获得的数据:4
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DELETE_SUCCESS">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							完成删除:5
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INIT_INFO">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							初期画面显示:6
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="UPLOAD_SUCCESS">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							上传成功:7
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LOGIN_ERROR">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							用户名或者密码有错误:8
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SESSION_TIMEOUT">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							因为长时间没有操作，已自动退出:9
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DB_ERROR">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							数据库操作异常:10
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DELETE_ERROR">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							数据的删除失败:12
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OPEHIST_ERROR">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							登录完毕，但操作履历导出失败:14
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OPENAME_OVERLAP">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							输入的操作者名字已经被使用。请输入与其不同的操作者名字:17
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NO_OPE_REGIST">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							该操作者现在正在进行登录。对登录中的操作者不能进行删除、编辑:18
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NO_AUTHORITY">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							不能编辑、删除上位操作类别的操作者:19
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LOGIN_ERR_SAME_PC">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							来自同一台计算机的登录:24
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LOGIN_ERR_IP">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							不能获取有效的IP地址:25
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GENERAL_ERR">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							处理失败:26
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LOGIN_ERR_SESSION_MAX">
				<xs:annotation>
					<xs:documentation>
						<![CDATA[ 
							连接的用户超过了session的最大数:27
			  			]]> 
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="displayPropertyType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="none"/>
			<xs:enumeration value="read"/>
			<xs:enumeration value="normal"/>
			<xs:enumeration value="notvisibled"/>
		</xs:restriction>
	</xs:simpleType>
	
